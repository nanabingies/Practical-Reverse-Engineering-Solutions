;
; SampleB.inf
;

[Version]
Signature="$WINDOWS NT$"
Class=Sample ; TODO: edit Class
ClassGuid={78A1C341-4539-11d3-B88D-00C04FAD5171} ; TODO: edit ClassGuid
Provider=%ManufacturerName%
CatalogFile=SampleB.cat
DriverVer= ; TODO: set DriverVer in stampinf property pages
PnpLockDown=1

[DestinationDirs]
DefaultDestDir = 12
SampleB_Device_CoInstaller_CopyFiles = 11

; ================= Class section =====================

[ClassInstall32]
Addreg=SampleClassReg

[SampleClassReg]
HKR,,,0,%ClassName%
HKR,,Icon,,-5

[SourceDisksNames]
1 = %DiskName%,,,""

[SourceDisksFiles]
SampleB.sys  = 1,,
WdfCoInstaller$KMDFCOINSTALLERVERSION$.dll=1 ; make sure the number matches with SourceDisksNames

;*****************************************
; Install Section
;*****************************************

[Manufacturer]
%ManufacturerName%=Standard,NT$ARCH$

[Standard.NT$ARCH$]
%SampleB.DeviceDesc%=SampleB_Device, Root\SampleB ; TODO: edit hw-id

[SampleB_Device.NT]
CopyFiles=Drivers_Dir

[Drivers_Dir]
SampleB.sys

;-------------- Service installation
[SampleB_Device.NT.Services]
AddService = SampleB,%SPSVCINST_ASSOCSERVICE%, SampleB_Service_Inst

; -------------- SampleB driver install sections
[SampleB_Service_Inst]
DisplayName    = %SampleB.SVCDESC%
ServiceType    = 1               ; SERVICE_KERNEL_DRIVER
StartType      = 3               ; SERVICE_DEMAND_START
ErrorControl   = 1               ; SERVICE_ERROR_NORMAL
ServiceBinary  = %12%\SampleB.sys

;
;--- SampleB_Device Coinstaller installation ------
;

[SampleB_Device.NT.CoInstallers]
AddReg=SampleB_Device_CoInstaller_AddReg
CopyFiles=SampleB_Device_CoInstaller_CopyFiles

[SampleB_Device_CoInstaller_AddReg]
HKR,,CoInstallers32,0x00010000, "WdfCoInstaller$KMDFCOINSTALLERVERSION$.dll,WdfCoInstaller"

[SampleB_Device_CoInstaller_CopyFiles]
WdfCoInstaller$KMDFCOINSTALLERVERSION$.dll

[SampleB_Device.NT.Wdf]
KmdfService =  SampleB, SampleB_wdfsect
[SampleB_wdfsect]
KmdfLibraryVersion = $KMDFVERSION$

[Strings]
SPSVCINST_ASSOCSERVICE= 0x00000002
ManufacturerName="<Your manufacturer name>" ;TODO: Replace with your manufacturer name
ClassName="Samples" ; TODO: edit ClassName
DiskName = "SampleB Installation Disk"
SampleB.DeviceDesc = "SampleB Device"
SampleB.SVCDESC = "SampleB Service"
